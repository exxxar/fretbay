{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "bensampo/laravel-enum": "^2.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "grkamil/laravel-telegram-logging": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "irazasyed/telegram-bot-sdk": "^3.4",
        "jackmartin/laravel-yandex-geocode": "^1.2",
        "kutia-software-company/larafirebase": "^1.3",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.4",
        "mariuzzo/laravel-js-localization": "^1.8",
        "pusher/pusher-php-server": "^4.1",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/twitter": "^4.1",
        "socialiteproviders/vkontakte": "^4.2",
        "spatie/laravel-translatable": "^4.6",
        "tcg/voyager": "^1.4",
        "vkcom/vk-php-sdk": "^5.101"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel-shift/blueprint": "^1.24",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "doctrine/dbal": "^2.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}